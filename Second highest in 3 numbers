#include<stdio.h>

int main() {
    int a, b, c;

    a = 9;
    b = 10;
    c = 1;

    if ((a > b && a < c) || (a > c && a < b)) {
        printf("%d is the second highest\n", a);
    } else if ((b > a && b < c) || (b > c && b < a)) {
        printf("%d is the second highest\n", b);
    } else {
        printf("%d is the second highest\n", c);
    }

    return 0;
}
